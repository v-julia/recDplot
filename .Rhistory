dist_to_df = function(dist1, st1, e1, dist2, st2, e2){
d1 = dist2list(dist1)
d2 = dist2list(dist2)
d = merge(d1, d2, by=c("row", "col"))
colnames(d)[3] = paste(toString(st1),toString(e1),sep=":")
colnames(d)[4] = paste(toString(st2),toString(e2),sep=":")
return(d)
}
